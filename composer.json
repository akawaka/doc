{
    "name": "akawaka/doc",
    "description": "The Akawaka Documentation Hub",
    "type": "project",
    "authors": [
        {
            "name": "fmatsos"
        }
    ],
    "require": {
      "cecil/theme-docs": "2.40.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "allow-plugins": {
        "cecil/theme-installer": true
      }
    },
    "scripts": {
        "post-autoload-dump": [
          "mkdir -p bin",
          "curl -Lo bin/cecil https://cecil.app/cecil.phar",
          "chmod +x bin/cecil"
        ]
    }
}
